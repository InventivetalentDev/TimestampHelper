{
  "name": "TimestampHelper",
  "version": "1.0.1",
  "description": "Display human-readable timestamps from selected unix timestamps",
  "author": "Haylee Sch√§fer (inventivetalent)",
  "icons": {
    "32": "img/clock-stamp-x32.png",
